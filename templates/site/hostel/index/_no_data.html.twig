{% set data = config.booker.bookingData %}

<section class="card mb-5">
    <div class="card-body py-4 text-center">
        <div class="mb-4">
            <i class="fas fa-search fa-2x"></i>
        </div>
        <div class="mb-4">
            <h2 class="h2-responsive font-weight-stone-500">Aucun résultat</h2>
        </div>
        <div class="">
            <a href="{{ path('app_hostel_index', {
                'adult': data.adult,
                'children': data.children,
                'checkin': data.duration.checkin,
                'checkout': data.duration.checkout,
                'location': data.location,
            }) }}" class="btn btn-primary btn-md">Réinitialiser</a>
        </div>
    </div>
</section>